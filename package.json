{
  "name": "exentriq-bootstrapui-v5",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-css": "sass --load-path=node_modules scss/styles.scss dist/styles.css",
    "autoprefixer": "postcss dist/styles.css --use autoprefixer -o dist/styles.css",
    "build-css-autoprefixer": "npm run build-css && npm run autoprefixer",
    "build-react": "cd react-component && npm run build",
    "watch-css": "nodemon -e scss -x \"npm run build-css-autoprefixer && cd react-component && npm run build\" --config nodemon.json",
    "start-server": "PORT=8000 nodemon server.js --config nodemon.json",
    "start": "concurrently \"npm run watch-css\" \"npm run start-server\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "bootstrap": "^5.3.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.42.1"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.22"
  }
}
